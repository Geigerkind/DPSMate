				</FontString>
				<FontString name="$parent_Value" inherits="GameFontNormalSmall" text="500000 (20.43%)">
					<Size x="70" y="24" />
					<Anchors>
						<Anchor point="RIGHT">
							<Offset x="-5" y="0" />
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
			<Layer level="BACKGROUND">
				<Texture name="$parent_selected">
					<Anchors>
						<Anchor point="TOPLEFT" />
						<Anchor point="BOTTOMRIGHT" />
					</Anchors>
					<Color r="1" g="1" b="1" a="0.5" />
				</Texture>
			</Layer>
		</Layers>
		<Scripts>
			<OnClick>
				if this:GetParent():GetParent():GetName()=="DPSMate_Details_CompareEHealing" then
					if this:GetParent():GetName() == "DPSMate_Details_CompareEHealing_player" then
						DPSMate.Modules.DetailsEHealing:PlayerSpells_Update(this:GetID(), "Compare")
						DPSMate.Modules.DetailsEHealing:SelectDetails_EHealingButton(1, "Compare")
					else
						DPSMate.Modules.DetailsEHealing:SelectDetails_EHealingButton(this:GetID(), "Compare")
					end
				else
					if this:GetParent():GetName() == "DPSMate_Details_EHealing_player" then
						DPSMate.Modules.DetailsEHealing:PlayerSpells_Update(this:GetID(), "")
						DPSMate.Modules.DetailsEHealing:SelectDetails_EHealingButton(1, "")
					else
						DPSMate.Modules.DetailsEHealing:SelectDetails_EHealingButton(this:GetID(), "")
					end
				end
			</OnClick>
		</Scripts>
	</Button>
	
