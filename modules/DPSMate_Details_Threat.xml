	<Button name="DPSMate_Details_Threat_ScrollButton" virtual="true">
		<Size x="220" y="24" />
		<Layers>
			<Layer level="ARTWORT">
				<Texture name="$parent_Icon" file="Interface\AddOns\DPSMate\images\dummy">
					<Size x="24" y="24" />
					<Anchors>
						<Anchor point="TOPLEFT" />
					</Anchors>
				</Texture>
				<FontString name="$parent_Name" inherits="GameFontNormalSmall" text="Sinister Strike Eviscrate" justifyH="LEFT">
					<Size x="110" y="24" />
					<Anchors>
						<Anchor point="LEFT" relativeTo="$parent_Icon" relativePoint="RIGHT">
							<Offset x="5" y="0" />
						</Anchor>
					</Anchors>
				</FontString>
				<FontString name="$parent_Value" inherits="GameFontNormalSmall" text="500000 (20.43%)">
					<Size x="70" y="24" />
					<Anchors>
						<Anchor point="RIGHT">
							<Offset x="-5" y="0" />
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
			<Layer level="BACKGROUND">
				<Texture name="$parent_selected">
					<Anchors>
						<Anchor point="TOPLEFT" />
						<Anchor point="BOTTOMRIGHT" />
					</Anchors>
					<Color r="1" g="1" b="1" a="0.5" />
				</Texture>
			</Layer>
		</Layers>
		<Scripts>
			<OnClick>
				if this:GetParent():GetParent():GetName()=="DPSMate_Details_CompareThreat" then
					if this:GetParent():GetName()=="DPSMate_Details_CompareThreat_LogCreature" then
						DPSMate.Modules.DetailsThreat:SelectCreatureButton(this:GetID(), "Compare")
					else
						DPSMate.Modules.DetailsThreat:SelectDetailsButton(DPSMate_Details_CompareThreat_Log_ScrollFrame.index, this:GetID(), "Compare")
					end
				else
					if this:GetParent():GetName()=="DPSMate_Details_Threat_LogCreature" then
						DPSMate.Modules.DetailsThreat:SelectCreatureButton(this:GetID(), "")
					else
						DPSMate.Modules.DetailsThreat:SelectDetailsButton(DPSMate_Details_Threat_Log_ScrollFrame.index, this:GetID(), "")
					end
				end
			</OnClick>
		</Scripts>
	</Button>
	
