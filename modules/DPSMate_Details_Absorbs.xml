	<Button name="DPSMate_Details_Absorbs_ScrollButton" virtual="true">
		<Size x="220" y="24" />
		<Layers>
			<Layer level="ARTWORT">
				<Texture name="$parent_Icon" file="Interface\AddOns\DPSMate\images\dummy">
					<Size x="24" y="24" />
					<Anchors>
						<Anchor point="TOPLEFT" />
					</Anchors>
				</Texture>
				<FontString name="$parent_Name" inherits="GameFontNormalSmall" justifyH="LEFT">
					<Size x="110" y="24" />
					<Anchors>
						<Anchor point="LEFT" relativeTo="$parent_Icon" relativePoint="RIGHT">
							<Offset x="5" y="0" />
						</Anchor>
					</Anchors>
				</FontString>
				<FontString name="$parent_Value" inherits="GameFontNormalSmall">
					<Size x="70" y="24" />
					<Anchors>
						<Anchor point="RIGHT">
							<Offset x="-5" y="0" />
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
			<Layer level="BACKGROUND">
				<Texture name="$parent_selected">
					<Anchors>
						<Anchor point="TOPLEFT" />
						<Anchor point="BOTTOMRIGHT" />
					</Anchors>
					<Color r="1" g="1" b="1" a="0.5" />
				</Texture>
			</Layer>
		</Layers>
		<Scripts>
			<OnClick>
				if this:GetParent():GetParent():GetName()=="DPSMate_Details_Absorbs" then
					local pa, pt = DPSMate_Details_Absorbs_Log_ScrollFrame, DPSMate_Details_Absorbs_LogTwo_ScrollFrame
					if this:GetParent():GetName()=="DPSMate_Details_Absorbs_LogCreature" then
						DPSMate.Modules.DetailsAbsorbs:SelectCreatureButton(this:GetID(), "")
					elseif this:GetParent():GetName()=="DPSMate_Details_Absorbs_Log" then
						DPSMate.Modules.DetailsAbsorbs:SelectCauseButton(pa.index, this:GetID(), "")
					elseif this:GetParent():GetName()=="DPSMate_Details_Absorbs_LogTwo" then
						DPSMate.Modules.DetailsAbsorbs:SelectCauseABButton(pa.index, pt.index, this:GetID(), "")
					end
				else
					local pa, pt = DPSMate_Details_CompareAbsorbs_Log_ScrollFrame, DPSMate_Details_CompareAbsorbs_LogTwo_ScrollFrame
					if this:GetParent():GetName()=="DPSMate_Details_CompareAbsorbs_LogCreature" then
						DPSMate.Modules.DetailsAbsorbs:SelectCreatureButton(this:GetID(), "Compare")
					elseif this:GetParent():GetName()=="DPSMate_Details_CompareAbsorbs_Log" then
						DPSMate.Modules.DetailsAbsorbs:SelectCauseButton(pa.index, this:GetID(), "Compare")
					elseif this:GetParent():GetName()=="DPSMate_Details_CompareAbsorbs_LogTwo" then
						DPSMate.Modules.DetailsAbsorbs:SelectCauseABButton(pa.index, pt.index, this:GetID(), "Compare")
					end
				end
			</OnClick>
		</Scripts>
	</Button>
	
